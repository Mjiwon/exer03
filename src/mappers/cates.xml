<?xml version="1.0" encoding="UTF-8" ?> <!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cates">
	
	<select id="selCate" resultType="java.util.HashMap">
		select * from cate
	</select>
	
	<select id="joinCates">
		select cates, count(cate) from (select * from issue full join cate on issue.cate = cate.cates) group by cates
	</select>
	
</mapper>